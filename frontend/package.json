{
  "name": "stardustengine-frontend",
  "version": "3.0.0",
  "private": true,
  "description": "StardustEngine - Next-generation NFT Gaming Platform on MultiversX",
  "keywords": ["nft", "gaming", "multiversx", "blockchain", "web3"],
  "author": "George Pricop",
  "homepage": "https://gzeu.github.io/StardustEngine",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gzeu/StardustEngine.git",
    "directory": "frontend"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:github": "GITHUB_PAGES=true NEXT_PUBLIC_BASE_PATH=/StardustEngine next build",
    "build:vercel": "NEXT_PUBLIC_BASE_PATH= next build",
    "start": "next start",
    "export": "next export",
    "predeploy": "npm run build:github",
    "deploy": "gh-pages -d out -t true",
    "deploy:github": "npm run build:github && npm run export && gh-pages -d out -t true",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "health-check": "curl -f http://localhost:3000/api/health || echo 'Health check failed'",
    "size-limit": "size-limit",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "@multiversx/sdk-dapp": "^5.0.11",
    "@multiversx/sdk-core": "^14.0.0",
    "@multiversx/sdk-wallet-connect-provider": "^3.0.0",
    "@multiversx/sdk-dapp-ui": "^0.0.30",
    "@tanstack/react-query": "^5.59.0",
    "@heroicons/react": "^2.1.5",
    "clsx": "^2.1.1",
    "framer-motion": "^11.5.4",
    "lucide-react": "^0.417.0",
    "next": "14.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "@react-three/fiber": "^8.15.12",
    "@react-three/drei": "^9.88.13",
    "three": "^0.158.0",
    "@react-three/postprocessing": "^2.15.11",
    "lottie-react": "^2.4.0",
    "react-spring": "^9.7.3",
    "@use-gesture/react": "^10.3.0",
    "react-intersection-observer": "^9.5.3",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "zustand": "^4.4.6",
    "recharts": "^2.8.0",
    "react-confetti": "^6.1.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-progress": "^1.0.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.8",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.158.0",
    "@types/react-virtualized": "^9.21.29",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.8",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.45",
    "size-limit": "^11.1.5",
    "@lhci/cli": "^0.14.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "webpack-bundle-analyzer": "^4.10.2",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=20.13.1",
    "npm": ">=10.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/*.js",
      "limit": "500 KB"
    }
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ]
  }
}